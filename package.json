{
	"name": "hidefiles",
	"displayName": "HideFiles",
	"description": "User configurable profiles that hide certain files.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.57.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:hidefiles.reloadConfig"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "hidefiles.reloadConfig",
				"title": "HideFiles: Reload Configuration"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.4",
		"@types/mocha": "^8.2.3",
		"@types/node": "^14.17.5",
		"@types/vscode": "^1.58.1",
		"@typescript-eslint/eslint-plugin": "^4.28.4",
		"@typescript-eslint/parser": "^4.28.4",
		"eslint": "^7.31.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.5",
		"vscode-test": "^1.6.1"
	},
	"publisher": "JeremyFunk",
	"dependencies": {
		"dat.gui": "^0.7.7"
	},
	"repository": {
		"url": "https://github.com/JeremyFunk/hidefiles"
	}
}
